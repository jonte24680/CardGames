    "just-clone": "^5.0.1",
    "socket.io": "^3.0.4",
    "systemjs": "^6.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/socket.io": "^2.1.13",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2"
  }
